   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "print('We''ll do some math ...\\n')\n",
    "x = int(input('Enter an integer: '))\n",
    "print('Powers of', x,'from 0 to 10')\n",
    "for i in range(11):\n",
    "    print(f'{i:4} {x**i}')\n",
    "\n",
    "print('\\nroots of', x,'from 1 to 10\\n')\n",
    "for i in range(1,11):\n",
    "    print(f'{i:4} {x**(1/i):12.5f}')\n",
    "\n",
    "print('\\nEnd of demo')\n",
    "\n"
   ]
  }
